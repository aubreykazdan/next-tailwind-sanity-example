{
  "root": true,
  "project": {
    "name": "sanity-example"
  },
  "api": {
    "projectId": "2qljgzuz",
    "dataset": "production"
  },
  "plugins": [
    "@sanity/base",
    "@sanity/default-layout",
    "@sanity/default-login",
    "@sanity/desk-tool",
    "inline-svg",
    "@sanity/dashboard",
    "better-slug",
    "@sanity/color-input",
    "mux-input"
  ],
  "env": {
    "development": {
      "plugins": ["@sanity/vision"]
    }
  },
  "parts": [
    {
      "name": "part:@sanity/base/schema",
      "path": "./schemas/schema"
    },
    {
      "name": "part:@sanity/desk-tool/structure",
      "path": "./deskStructure.js"
    }
  ]
}
